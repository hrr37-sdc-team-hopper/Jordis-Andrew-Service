  "scripts": {
    "pg-start": "pg_ctl -D /usr/local/var/postgres start",
    "pg-stop": "pg_ctl -D /usr/local/var/postgres stop",
    "seed-pg": "psql booksInfo < pgSchema.sql",
    "seed": "mongoimport -d bookInfo -c books --type csv --file database/csvFiles/mockData10M.csv --headerline",
    "drop:tables": "node database/dropTables.js",
    "start": "nodemon server/server.js",
    "build:dev": "webpack -d --watch --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:server": "jest server",
    "test:client": "jest client"
  },
  "dependencies": {
